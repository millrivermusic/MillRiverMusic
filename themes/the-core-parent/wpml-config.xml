<wpml-config>
    <custom-types>
        <custom-type translate="0">fw_backup</custom-type>
        <custom-type translate="1">fw-event</custom-type>
        <custom-type translate="1">fw-portfolio</custom-type>
        <custom-type translate="1">fw-slider</custom-type>
        <custom-type translate="1">fw-learning-articles</custom-type>
        <custom-type translate="1">fw-learning-courses</custom-type>
        <custom-type translate="1">fw-learning-quiz</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="1">fw-event-taxonomy-name</taxonomy>
        <taxonomy translate="1">fw-portfolio-category</taxonomy>
        <taxonomy translate="1">fw-learning-courses-taxonomy</taxonomy>
    </taxonomies>
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#dddddd</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#dddddd</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_language_switcher_sidebar">0</key>
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>
    </language-switcher-settings>
    <admin-texts>
        <key name="fw_theme_settings_options:the-core">
            <key name="logo_settings">
                <key name="logo">
                    <key name="image">
                        <key name="image_logo">
                            <key name="url"/>
                        </key>
                    </key>
                </key>
            </key>
        </key>
        <key name="fw_theme_settings_options:the-core">
            <key name="footer_settings">
                <key name="copyright"/>
            </key>
            <key name="header_settings">
                <key name="enable_header_top_bar">
                    <key name="yes">
                        <key name="header_text"/>
                    </key>
                </key>
            </key>
        </key>
    </admin-texts>
</wpml-config>